	"license": "MIT",
	"main": "dist/index.cjs.js",
	"module": "dist/index.js",
	"name": "@limeyfy/payload-plugin-map-point",
	"peerDependencies": {
		"@googlemaps/js-api-loader": ">=1.16.0",
		"leaflet": ">=1.9.0",
