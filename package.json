{
  "name": "@limeyfy/payload-plugin-map-point",
  "version": "1.0.7",
  "description": "Payload plugin to enhance point fields with a Mapbox map UI and optional geocoding.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./admin/MapPointField": {
      "types": "./dist/admin/MapPointField.d.ts",
      "import": "./dist/admin/MapPointField.js",
      "require": "./dist/admin/MapPointField.cjs.js"
    },
    "./admin/ClientMapPointField": {
      "types": "./dist/admin/ClientMapPointField.d.ts",
      "import": "./dist/admin/ClientMapPointField.js",
      "require": "./dist/admin/ClientMapPointField.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "release": "release-it"
  },
  "keywords": [
    "payload",
    "payloadcms",
    "plugin",
    "mapbox",
    "geocoder",
    "point"
  ],
  "author": "",
  "peerDependencies": {
    "payload": "^3.0.0",
    "react": ">=18",
    "react-dom": ">=18",
    "mapbox-gl": ">=3.5.0"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^3.4.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@release-it/conventional-changelog": "^8.0.2",
    "release-it": "^17.6.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}